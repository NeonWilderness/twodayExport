{
  "name": "twodayexport",
  "version": "1.7.0",
  "description": "Blog Export on steroids",
  "main": "twoday-export.html",
  "scripts": {
    "build": "gulp --production",
    "dev": "gulp",
    "bump": "gulp bump",
    "date": "node ./tools/checkDates --blog=blogalias --dir=../clients/",
    "deploy": "node ./utils/uploadSkin --alias=talkstraight --platform=prod",
    "hint": "gulp hint",
    "init": "node ./tools/resInit --blog=blogalias",
    "pdf": "node ./tools/resHtml2Pdf --blog=blogalias --dir=../clients/",
    "release": "gulp release",
    "resources": "node ./tools/resResources --blog=blogalias --dir=../clients/",
    "split:td": "node ./tools/resSplitFile --blog=blogalias --dir=../clients/",
    "split:wp": "node ./tools/resSplitWP --blog=blogalias --dir=../clients/",
    "static": "node ./tools/resStaticRefs --blog=blogalias",
    "lower": "node ./tools/resFixUpper --blog=blogalias",
    "download": "node ./tools/resDownload --blog=blogalias --dir=../clients/ --download",
    "test": "mocha ./tools/checkExport --blog=blogalias --dir=../clients/ --reporter progress --require mocha-clean",
    "missing": "node ./tools/resMissingIDs --blog=blogalias --dir=../clients/",
    "wp": "node ./tools/resWordpress --blog=blogalias --dir=../clients/ --notfound=list",
    "wpcat": "node ./tools/resWPCat --blog=blogalias",
    "wpdel:post": "node ./tools/resWPDelete --blog=blogalias",
    "wpdel:trash": "node ./tools/resWPDelete --blog=blogalias --trash",
    "wpfix:links": "node ./tools/resWPFixLinks --blog=blogalias --dir=../clients/ --noupdate",
    "wpfix:img": "node ./tools/resWPFixImg --blog=blogalias --noupdate",
    "wpfix:vids": "node ./tools/resWPFixVids --blog=blogalias --noupdate"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@hapi/hoek": "^9.2.1",
    "@neonwilderness/twoday": "^0.3.14",
    "@types/chai": "^4.2.22",
    "@types/jquery": "^3.5.7",
    "@types/mocha": "^9.0.0",
    "ansi-colors": "^4.1.1",
    "chai": "^4.3.4",
    "cheerio": "^1.0.0-rc.10",
    "cp-file": "^9.1.0",
    "dotenv-safe": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^2.0.7",
    "gulp-replace": "^1.1.3",
    "isuri": "^2.0.3",
    "jshint": "^2.13.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^9.1.2",
    "mocha-clean": "^1.0.0",
    "mustache": "^4.2.0",
    "puppeteer": "^10.4.0",
    "semver": "^7.3.5",
    "wordpress": "^1.4.1",
    "yargs": "^17.2.1"
  }
}
