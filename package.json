{
  "name": "twodayexport",
  "version": "1.3.0",
  "description": "Blog Export on steroids",
  "main": "twoday-export.html",
  "scripts": {
    "build": "gulp4 --production",
    "dev": "gulp4",
    "bump": "gulp4 bump",
    "deploySkin": "node ./utils/uploadSkin --blog=kunstbetrieb",
    "deployVersion": "node ./utils/uploadFile --blog=neonwilderness",
    "hint": "gulp4 hint",
    "init": "node ./tools/resInit --blog=kunstbetrieb",
    "pdf": "node ./tools/resHtml2Pdf --blog=kunstbetrieb --dir=../clients/",
    "release": "gulp4 release",
    "resources": "node ./tools/resResources --blog=kunstbetrieb --dir=../clients/",
    "split:td": "node ./tools/resSplitFile --blog=kunstbetrieb --dir=../clients/",
    "split:wp": "node ./tools/resSplitWP --blog=kunstbetrieb --dir=../clients/",
    "static": "node ./tools/resStaticRefs --blog=kunstbetrieb",
    "download": "node ./tools/resDownload --blog=kunstbetrieb --dir=../clients/ --download",
    "test": "mocha ./tools/checkExport --blog=kunstbetrieb --dir=../clients/ --reporter progress --require mocha-clean",
    "missing": "node ./tools/resMissingIDs --blog=kunstbetrieb --dir=../clients/",
    "wp": "node ./tools/resWordpress --blog=kunstbetrieb --dir=../clients/ --notfound=list",
    "wpcat": "node ./tools/resWPCat --blog=kunstbetrieb",
    "wpdel:post": "node ./tools/resWPDelete --blog=kunstbetrieb",
    "wpdel:trash": "node ./tools/resWPDelete --blog=kunstbetrieb --trash",
    "wpfix:links": "node ./tools/resWPFixLinks --blog=kunstbetrieb --dir=../clients/ --noupdate",
    "wpfix:img": "node ./tools/resWPFixImg --blog=kunstbetrieb --noupdate"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/jquery": "^3.3.1",
    "@types/mocha": "^2.2.48",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "cp-file": "^5.0.0",
    "dotenv-safe": "^5.0.1",
    "gulp-bump": "^3.1.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.6.1",
    "gulp-util": "^3.0.8",
    "gulp4": "^4.0.2",
    "isuri": "^2.0.3",
    "jshint": "^2.9.5",
    "jshint-stylish": "^2.2.1",
    "mocha": "^5.1.0",
    "mocha-clean": "^1.0.0",
    "mustache": "^2.3.0",
    "puppeteer": "^1.3.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "semver": "^5.5.0",
    "wordpress": "^1.4.1",
    "yargs": "^11.1.0"
  },
  "dependencies": {}
}
