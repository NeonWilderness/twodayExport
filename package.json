{
  "name": "twodayexport",
  "version": "0.99.0",
  "description": "Blog Export on steroids",
  "main": "twoday-export.html",
  "scripts": {
    "build": "gulp4 --production",
    "dev": "gulp4",
    "bump": "gulp4 bump",
    "deploySkin": "node ./utils/uploadSkin --blog=talkstraight",
    "deployVersion": "node ./utils/uploadFile --blog=talkstraight",
    "hint": "gulp4 hint",
    "release": "gulp4 release",
