    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cp-file": "^5.0.0",
    "dotenv-safe": "^5.0.1",
    "gulp-bump": "^3.1.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.6.1",
    "gulp-util": "^3.0.8",
    "gulp4": "^4.0.2",
    "isuri": "^2.0.3",
    "jshint": "^2.9.5",
    "jshint-stylish": "^2.2.1",
    "mocha": "^5.0.1",
    "mustache": "^2.3.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "semver": "^5.5.0",
